<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="cn.thinkjoy.dao.mapper.IDataModel2Mapper">
	<resultMap id="rm" type="cn.thinkjoy.dao.beans.IDataModel2">
		<result property="id" jdbcType="INTEGER" column="id"/>
		<result property="creator" jdbcType="INTEGER" column="creator"/>
		<result property="createDate" jdbcType="BIGINT" column="createDate"/>
		<result property="lastModifier" jdbcType="INTEGER" column="lastModifier"/>
		<result property="lastModDate" jdbcType="BIGINT" column="lastModDate"/>
		<result property="status" jdbcType="TINYINT" column="status"/>
		<result property="priority" jdbcType="INTEGER" column="priority"/>
		<result property="modelId" jdbcType="INTEGER" column="modelId"/>
		<result property="assignUrl" jdbcType="VARCHAR" column="assignUrl"/>
		<result property="whereSql" jdbcType="VARCHAR" column="whereSql"/>
		<result property="name" jdbcType="VARCHAR" column="name"/>
	</resultMap>
	<sql id="Base_Column_List">
		id,
		creator,
		createDate,
		lastModifier,
		lastModDate,
		status,
		priority,
		modelId,
		assignUrl,
		whereSql,
		name
	</sql>
	<select id="load" parameterType="java.lang.Integer" resultMap="rm">
		SELECT 
			<include refid="Base_Column_List" />
		FROM cmc1_data_model2 
		WHERE 
			id = #{id} 
	</select>
	<!-- 查询所有的ID -->
	<select id="findAllIds" resultType="java.lang.Integer">
		SELECT id FROM cmc1_data_model2
	</select>

	<!-- 按Id删除 -->
	<delete id="delete" parameterType="java.lang.Integer">
		DELETE FROM cmc1_data_model2 WHERE
		id = #{id} 
	</delete>
	 <!-- 插入 -->
	<insert id="insert" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO cmc1_data_model2 (
			id,
			creator,
			createDate,
			lastModifier,
			lastModDate,
			status,
			priority,
			modelId,
			assignUrl,
			whereSql,
			name
		) VALUES (
			#{id},
			#{creator},
			#{createDate},
			#{lastModifier},
			#{lastModDate},
			#{status},
			#{priority},
			#{modelId},
			#{assignUrl},
			#{whereSql},
			#{name}
		)
	</insert>
	
	<select id="countAll" resultType="java.lang.Integer">
		SELECT count(id) FROM cmc1_data_model2	
	</select>
	
	<select id="findAll" resultMap="rm">
		SELECT 
			<include refid="Base_Column_List" />
		FROM cmc1_data_model2
	</select>
	
	<!-- 更新 -->
	<update id="updateSelective" parameterType="cn.thinkjoy.dao.beans.IDataModel2">
		UPDATE cmc1_data_model2 
			<set>
				<if test="creator!=null">
				creator = #{creator},
				</if>
				<if test="createDate!=null">
				createDate = #{createDate},
				</if>
				<if test="lastModifier!=null">
				lastModifier = #{lastModifier},
				</if>
				<if test="lastModDate!=null">
				lastModDate = #{lastModDate},
				</if>
				<if test="status!=null">
				status = #{status},
				</if>
				<if test="priority!=null">
				priority = #{priority},
				</if>
				<if test="modelId!=null">
				modelId = #{modelId},
				</if>
				<if test="assignUrl!=null">
				assignUrl = #{assignUrl},
				</if>
				<if test="whereSql!=null">
				whereSql = #{whereSql},
				</if>
				<if test="name!=null">
				name = #{name}
				</if>
			</set>
		WHERE 
			id = #{id}
	</update>
	
	<update id="update" parameterType="cn.thinkjoy.dao.beans.IDataModel2">
		UPDATE cmc1_data_model2 SET
			creator = #{creator},
			createDate = #{createDate},
			lastModifier = #{lastModifier},
			lastModDate = #{lastModDate},
			status = #{status},
			priority = #{priority},
			modelId = #{modelId},
			assignUrl = #{assignUrl},
			whereSql = #{whereSql},
			name = #{name}
		WHERE 
			id = #{id} 
	</update>
	
</mapper>

