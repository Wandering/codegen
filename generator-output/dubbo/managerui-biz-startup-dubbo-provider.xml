<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mybatis="http://mybatis.org/schema/mybatis-spring"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <description>managerui-biz-startup dubbo 服务提供配置 </description>

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="managerui-biz-startup"  />
    <!-- 注册中心暴露服务地址 -->
    <!--<dubbo:registry address="zookeeper://10.172.7.71:2181" />-->
    <dubbo:registry address="${dubbo_zk_url}" file="/tmp/dubbo.cache"/>

    <bean id="DataModel2ServiceImpl" class="cn.thinkjoy.service.impl.DataModel2ServiceImpl" />
    <dubbo:service id="datamodel2Service" interface="cn.thinkjoy.service.IDataModel2Service" ref="DataModel2ServiceImpl"/>

    <bean id="DataModel2FacadeImpl" class="cn.thinkjoy.facade.impl.DataModel2FacadeImpl" />
    <dubbo:service id="datamodel2Facade" interface="cn.thinkjoy.facade.IDataModel2Facade" ref="DataModel2FacadeImpl"/>

</beans>